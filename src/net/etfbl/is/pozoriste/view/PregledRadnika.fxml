               </VBox.margin>
            </Button>
            <Button fx:id="bPretrazi" mnemonicParsing="false" onAction="#pretraziRadnikaAction" text="Pretrazi">
               <VBox.margin>
                  <Insets left="10.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
      <TextField fx:id="tfPretraga" layoutX="150.0" layoutY="102.0" prefHeight="25.0" prefWidth="262.0" />
